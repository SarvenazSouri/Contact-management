<Window x:Class="view.Rechercher"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:view"
        mc:Ignorable="d"
        Title="Rechercher" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textbox1" HorizontalAlignment="Left" Height="52" Margin="62,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="470" TextChanged="TextBox_TextChanged"/>
        <Button Content="search" HorizontalAlignment="Left" Margin="581,42,0,0" VerticalAlignment="Top" Width="126" Height="52" FontSize="20" FontWeight="Bold" FontFamily="Times New Roman" Click="Button_Click_1"/>
        <Label x:Name="label1" Content="" HorizontalAlignment="Left" Margin="62,129,0,0" VerticalAlignment="Top" Height="229" Width="470"/>
        <Button Content="Return" HorizontalAlignment="Left" Margin="581,326,0,0" VerticalAlignment="Top" Width="126" Height="52" FontSize="20" FontWeight="Bold" FontFamily="Times New Roman" Click="Button_Click"/>
        <Label Content="Nom ou Prenom" HorizontalAlignment="Left" Height="32" Margin="62,10,0,0" VerticalAlignment="Top" Width="199" FontSize="16"/>
    </Grid>
</Window>
